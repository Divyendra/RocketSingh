<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DrawerIcon">
        <item name="android:layout_height">@dimen/drawer_icon_size</item>
        <item name="android:layout_width">@dimen/drawer_icon_size</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:clickable">false</item>
        <!--<item name="android:tint">@color/drawer_icon_tint</item>-->
    </style>

    <style name="DrawerText">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textAppearance">?android:attr/textAppearance</item>
        <item name="android:textColor">@color/drawer_text</item>
        <item name="android:textSize">@dimen/text_sz_xlarge</item>
        <item name="android:clickable">false</item>
    </style>

</resources>